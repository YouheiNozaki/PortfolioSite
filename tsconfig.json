{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "strict": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "lib": ["dom", "esnext"]
  },
  "exclude": ["node_modules", ".cache"],
  "include": ["./gatsby-node/**/*", "./src/**/*"]
}
