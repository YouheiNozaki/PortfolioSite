{
  "name": "gatsby-starter-typescript",
  "description": "Gatsby TypeScript starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "autoprefixer": "^9.8.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "gatsby": "^2.20.29",
    "gatsby-image": "^2.4.3",
    "gatsby-link": "^2.3.4",
    "lottie-web": "^5.6.10",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^6.0.0",
    "tailwindcss": "^1.4.6",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "gatsby, typescript"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "gatsby build",
    "start": "gatsby develop",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}*\"",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@types/node": "^13.13.2",
    "@types/prettier": "^2.0.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^5.0.14",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-preset-gatsby": "^0.4.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-manifest": "^2.3.6",
    "gatsby-plugin-offline": "^3.2.2",
    "gatsby-plugin-postcss": "^2.3.1",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sharp": "^2.5.6",
    "gatsby-plugin-typescript": "^2.4.0",
    "gatsby-source-filesystem": "^2.2.4",
    "gatsby-source-microcms": "^0.1.0",
    "gatsby-transformer-sharp": "^2.4.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.3",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
